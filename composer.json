{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0.3",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0|^2.0",
        "guzzlehttp/guzzle": "^6.3|^7.3.0",
        "laravel/framework": "^7.0|^8.37.0 ",
        "laravel/socialite": "v4.4.0|v4.4.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1.0|^2.5.0 ",
        "pusher/pusher-php-server": "^4.1",
        "spatie/laravel-newsletter": "^4.8",
        "srmklive/paypal": "~1.0",
        "unisharp/laravel-filemanager": "^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1|^5.4.0 ",
        "phpunit/phpunit": "^8.5|^9.5.4  "
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/http/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }

}
}
